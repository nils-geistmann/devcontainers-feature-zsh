{
  "using_non_root_remote_user": {
    "image": "debian:bookworm-slim",
    "features": {
      "ghcr.io/nils-geistmann/devcontainers-features/create-remote-user:latest": {},
      "zsh": {}
    },
    "remoteUser": "remote"
  },
  "using_theme_agnoster": {
    "image": "mcr.microsoft.com/devcontainers/base:bullseye",
    "features": {
      "zsh": {
        "theme": "agnoster"
      }
    }
  },
  "using_theme_agnoster_with_default_user": {
    "image": "mcr.microsoft.com/devcontainers/base:bullseye",
    "features": {
      "zsh": {
        "theme": "agnoster",
        "defaultUser": "vscode"
      }
    }
  },
  "using_theme_agnoster_with_no_default_user": {
    "image": "mcr.microsoft.com/devcontainers/base:bullseye",
    "features": {
      "zsh": {
        "theme": "agnoster"
      }
    }
  },
  "configuring_plugins_git_and_docker": {
    "image": "mcr.microsoft.com/devcontainers/base:bullseye",
    "features": {
      "zsh": {
        "plugins": "git docker"
      }
    }
  },
  "create_zshrc_file_if_missing": {
    "image": "debian:bookworm-slim",
    "features": {
      "zsh": {
        "theme": "avit",
        "plugins": "git docker"
      }
    }
  }
}
